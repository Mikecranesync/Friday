{
  "expo": {
    "name": "Friday",
    "slug": "friday",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0118"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.friday.voiceassistant"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0118"
      },
      "package": "com.friday.voiceassistant",
      "permissions": [
        "RECORD_AUDIO",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "704a3abf-0e2a-4dd2-a98a-185cfb03f355"
      }
    },
    "owner": "mikecranesync"
  }
}
